  <step>
    <name>generate variable date holidays</name>
    <type>ScriptValueMod</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <compatible>N</compatible>
    <optimizationLevel/>
    <jsScripts>
      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>// this script generates all variable date holidays 
// the variable date holidays depend on date of easter, so that is calculated first
// the other dates are derived from it

// calculates (western) easter sunday, details about easter finding algorithm: http://www.gmarts.org/index.php?go=415
function easterSunday() {
	var a = year % 19;
	var b = Math.floor(year/100);
	var c = year % 100;
	var d = Math.floor(b/4);
	var e = b % 4;
	var f = Math.floor((b+8) / 25);
	var g = Math.floor((b-f+1) / 3);
	var h = (19*a + b - d - g + 15) % 30;
	var i = Math.floor(c/4);
	var j = c % 4;
	var k = (32 + 2*e + 2*i - h - j) % 7;
	var m = Math.floor((a + 11*h + 22*k) / 451);
	var month = Math.floor((h + k - 7*m + 114) / 31);
	var day = ((h + k - 7*m +114) % 31) + 1;

	return new Date(year, month-1, day);

}

// puts a holiday to the row stream
function writeHoliday(holidayDate, holidayName){
	var row = createRowCopy(getOutputRowMeta().size());
	var idx = getInputRowMeta().size();
    row[idx++] = date2str(holidayDate, 'yyyy-MM-dd');
    row[idx++] = holidayName;
    
    putRow(row);
	
}

// generate variable holidays
writeHoliday(dateAdd(easterSunday(),"d",-2), "Karfreitag");
writeHoliday(easterSunday(), "Ostersonntag");
writeHoliday(dateAdd(easterSunday(),"d",1), "Ostermontag");
writeHoliday(dateAdd(easterSunday(),"d",39), "Christi Himmelfahrt");
writeHoliday(dateAdd(easterSunday(),"d",49), "Pfingstsonntag");
writeHoliday(dateAdd(easterSunday(),"d",50), "Pfingstmontag");
writeHoliday(dateAdd(easterSunday(),"d",60), "Fronleichnam");


// do not output the incoming row (which contains only the year field)
trans_Status = SKIP_TRANSFORMATION;
</jsScript_script>
      </jsScript>
    </jsScripts>
